function showLogin(e,n){var t=document.forms.login;if(document.getElementById&&t){if("undefined"!=typeof n&&(t.retpath.value=n),2==e){var o=getCookie("yandex_login");e=o&&o.length>1,e&&t.login.value!=o&&(t.login.value=o,t.passwd.value="")}return document.getElementById("login-form").style.display="block",e?t.passwd.focus():t.login.focus(),g_loginkeydown=document.onkeydown,document.onkeydown=function(e){e=e||window.event,27!=e.keyCode||e.ctrlKey||e.altKey||hideLogin()},!1}var r=new Image;return r.src="https://clck.yandex.ru/click/dtype=morda/*https://mail.yandex.ru/",!0}function hideLogin(){var e=document.forms.login;return e.passwd.value="",document.onkeydown=g_loginkeydown,document.getElementById("login-form").style.display="none",!1}function getCookie(e){var n=document.cookie;if(n.length<1)return!1;var t=n.indexOf(e+"=");if(-1==t)return!1;t+=e.length+1;var o=n.indexOf(";",t);return unescape(-1==o?n.substring(t):n.substring(t,o))}function settime(){document.forms.login.timestamp.value=(new Date).getTime()}function search_type(e){var n=document.forms.web;return n.text.value?(n.stype.value=e,n.submit(),!1):!0}function unescape_opera(e){return"function"==typeof RegExp?(re=/quot;/g,newstr=e.replace(re,""),re=/&/g,newstr.replace(re,'"')):e}function r(e,n,t){try{var o=(new Date).getTime(),r=e?e.href:document.location;document.createElement("IMG").src="https://clck.yandex.ru/click/dtype="+n+(t?"":"/u="+o+"/*"+r)}catch(u){return!0}}var g_loginkeydown;